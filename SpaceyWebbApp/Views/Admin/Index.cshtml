@model SpaceyWebbApp.Models.Expedition

@{
    ViewData["Title"] = "Index";
}

    <body id="background-image">

        <h1>Admin</h1>

        <a class="btn btn-primary" asp-area="" asp-controller="Admin" asp-action="AddRocket">Add a Rocket</a>
        <a class="btn btn-primary" asp-area="" asp-controller="Admin" asp-action="AddDestination">Add a Destination</a>
        <a class="btn btn-primary" asp-area="" asp-controller="Admin" asp-action="AddExpedition">Add a Expedition</a>
        <hr />


        <table class="table">
            <thead>
                <tr>
                    <th>Departure</th>
                    <th>Destination</th>
                    <th>Rocket</th>
                    <th>SeatsLeft</th>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>

        @section Scripts {
            <script src="~/js/expeditions/expedition.get.js"></script>
            <script src="~/js/expeditions/expedition.js"></script>
            @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
        }
    </body>
